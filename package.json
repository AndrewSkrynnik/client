{
  "name": "rotazap-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development bunx next dev",
    "dev:turbo": "NODE_ENV=development bunx next dev --turbopack",
    "build": "NODE_ENV=production bun run check:heavy && bun run lint && ANALYZE=true bunx next build",
    "postbuild": "bunx next-sitemap",
    "start": "NODE_ENV=production bunx next start",
    "lint": "bunx next lint",
    "lint:css": "bunx stylelint \"**/*.css\" --cache --allow-empty-input",
    "lint:css:fix": "bunx stylelint \"**/*.css\" --fix",
    "check:heavy": "bun scripts/find-heavy-components.mjs",
    "check:cycles": "bunx depcruise app --output-type err",
    "check:cycles:graph": "bunx depcruise app --output-type dot | dot -T svg > dep-graph.svg",
    "check:deps": "ncu",
    "upgrade:deps": "ncu -u && bun install"
  },
  "dependencies": {
    "next": "15.3.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@hookform/resolvers": "^5.0.1",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@next/bundle-analyzer": "^15.3.1",
    "@react-input/mask": "^2.0.4",
    "@tailwindcss/postcss": "^4.1.5",
    "@tanstack/react-query": "^5.75.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-date-range": "^1.4.10",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dependency-cruiser": "^16.10.1",
    "eslint": "^9.26.0",
    "eslint-config-next": "^15.3.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "gsap": "^3.13.0",
    "js-cookie": "^3.0.5",
    "next-sitemap": "^4.2.3",
    "npm-check-updates": "^18.0.1",
    "postcss": "^8.5.3",
    "postcss-functions": "^4.0.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-date-range": "^2.0.1",
    "react-hook-form": "^7.56.2",
    "react-scan": "^0.3.3",
    "react-toastify": "^11.0.5",
    "socket.io-client": "^4.8.1",
    "stylelint": "^16.19.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.0",
    "tailwindcss": "^4.1.5",
    "typescript": "^5",
    "yup": "^1.6.1",
    "zustand": "^5.0.4"
  }
}
